#include<stdio.h>
long long gunc(int k){
	long long num=1;
	for(int i=1;i<=k;i++){
		num *= i;
	}
	return num;
}
long long gunc2(int n,int m)//n>m
{
	long long k=1;
	for(int i=m+1;i<=n;i++){//计算阶乘时先尽量约分避免数据溢出
		k *= i;
	}
	return k;
}
long long func(int n,int m){
	if((n-m)>m)return gunc2(n,n-m)/gunc(m);
	if((n-m)<=m)return gunc2(n,m)/gunc(n-m);
}
int main()
{
	int n,m;
	long long a,b,c;
	scanf("%d %d",&n,&m);
	printf("%lld",func(n,m)); 
	return 0;
} 
