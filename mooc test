#include<stdio.h>
int su(int score[])
{
	int sum=0;
	for(int i=0;i<=9;i++)
	{
		sum += score[i];
	}
	printf("%d\n",sum);
	return sum;
}
int he(int score[])
{
	int max=score[0];
	for(int n=0;n<=8;n++)
	{
		if(score[n]>max)
		{
			max = score[n];
		}
	}
	return max;
}
int lo(int score[])
{
	int low=score[9];
	for(int n=9;n>=2;n--)
	{
		if(score[n]<low)
		{
			low = score[n];
		}
	}
	return low;
}
int ave(int score[])
{
	int average,sum1;
	for(int n=0;n<=9;n++)
	{
		sum1 += score[n];
	}
	average = sum1/10;
	return average;
}
int final(int score[])
{
	int i,j;
	for(i=0;i<=8;i++)
	{
		for(j=i;j<=9;j++)
		{
			if(score[i]<score[j])
			{
				int temp;
				temp = score[j];
				score[j] = score [i];
				score[i] = temp;
			}
		}
	}
	for(i=0;i<=9;i++)
	{
		if(i != 9)
		{
			printf("%d, ",score[i]);
		}
		else
		{
			printf("%d\n",score[i]);
		}
	}
}
int main()
{
    int score[]={67,98,75,63,82,79,81,91,66,84};
    printf("总和%d\n",su(score));
    printf("最大%d\n",he(score)); 
    printf("最小%d\n",lo(score)); 
    printf("平均%d\n",ave(score));
    final(score);
    return 0;
}
